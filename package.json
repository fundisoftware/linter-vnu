{
  "name": "linter-vnu",
  "main": "lib/index.js",
  "version": "0.0.0",
  "description": "Lint HTML5 or XHTML5 on the fly using the Nu Html Checker (v.Nu)",
  "keywords": [
  ],
  "repository": "https://github.com/fundisoftware/linter-vnu",
  "license": "MIT",
  "author": {
    "name": "Graham Hannington"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
	  "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "configSchema": {
    "vnuUrl": {
      "title": "v.Nu URL",
      "description": "`https://validator.nu/` is one of several public v.Nu URLs. Please do not use a public v.Nu for linting on the fly.",
      "type": "string",
      "default": "http://localhost:8888"
    }
  }
}
